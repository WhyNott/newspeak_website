- content_for :page_title do
  Full member graph

.page-header
  %h1 Full member graph

= form_tag(graphs_full_path, method: :get) do
  = select_date @start_date, prefix: :start, include_blank: true
  = select_date @end_date, prefix: :end, include_blank: true
  = submit_tag 'Show'

#sigma-container{ style: 'width: 100%; height: 600px; background: #eee' }

= render partial: 'graph'
